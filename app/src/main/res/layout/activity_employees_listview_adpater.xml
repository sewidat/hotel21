<?xml version="1.0" encoding="utf-8"?>

<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:stretchColumns="*"
    tools:context=".controller.AdminController.AdminRemoveEmployee">



    <TableRow>
        <TextView
            android:layout_column="0"
            android:layout_span="1"
            android:layout_marginLeft="20dp"
            android:id="@+id/Delete_useridID"
            android:layout_width="50dp"
            android:layout_height="50dp"
            />

        <TextView
            android:layout_marginLeft="30dp"
            android:layout_column="1"
            android:layout_span="1"
            android:id="@+id/Delete_first_nameID"
            android:layout_width="50dp"
            android:layout_height="50dp"
            />

        <TextView
            android:layout_column="3"
            android:layout_span="1"
            android:id="@+id/Delete_last_nameID"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            />


        <Button
            android:layout_marginRight="20dp"
            android:layout_column="2"
            android:layout_span="1"
            android:id="@+id/Delete_btnEmplyoee"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:layout_marginTop="5dp"
            android:onClick="OnclickForDoneReserve"
            android:textSize="10dp"


            />





    </TableRow>
</TableLayout>